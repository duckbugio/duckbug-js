{
  "name": "@duckbug/js",
  "version": "0.1.3",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      },
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rslib build",
    "lint": "bunx biome ci",
    "prepare": "husky",
    "test": "vitest run",
    "publish": "bun run build && npm publish"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.1",
    "@rslib/core": "0.10.6",
    "husky": "9.1.7",
    "pinst": "3.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "vitest": "^3.2.4"
  },
  "packageManager": "bun@latest"
}
